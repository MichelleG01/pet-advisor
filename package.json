{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll=false",
    "test-coverage": "jest --verbose --coverage  --testPathPattern=\".test.js\" --verbose --runInBand"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base |react-native-safe-area-context|@sentry/.*|rn-pdf-reader-js)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "~5.2.6",
    "@react-navigation/native": "~5.1.5",
    "@react-navigation/stack": "~5.2.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16.1": "^1.6.4",
    "expo": "~41.0.1",
    "expo-status-bar": "~1.0.4",
    "firebase": "~8.2.3",
    "jest-enzyme": "^7.1.2",
    "lodash": "^4.17.21",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
    "react-native-easy-toast": "~1.2.0",
    "react-native-elements": "~1.2.7",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-keyboard-aware-scroll-view": "~0.9.1",
    "react-native-reanimated": "~2.1.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.0.0",
    "react-native-web": "~0.13.12"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react-native": "^7.1.0",
    "babel-jest": "^26.6.3",
    "babel-preset-expo": "^8.3.0",
    "enzyme-to-json": "^3.6.1",
    "jest": "^26.6.3",
    "jest-expo": "^39.0.0",
    "react-test-renderer": "^16.13.1"
  },
  "private": true
}
